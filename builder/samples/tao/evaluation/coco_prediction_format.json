        "score": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "description": "Detection confidence score from detection_scores"
        },
        "category_id": {
          "type": "integer",
          "description": "Category identifier from detection_classes"
        }
      },
      "required": ["image_id", "bbox", "score", "category_id"]
    }
